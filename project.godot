;   param=value ; assign values to parameters

config_version=5

[application]

config/name="agile-clicker"
config/description="点击风暴，一个鼠标点击小游戏"
config/tags=PackedStringArray("2d")
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/image="res://assets/LiRenTechLogoTransparent.png"
config/icon="res://assets/icon.webp"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
mouse_cursor/custom_image="res://assets/mouse.png"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
